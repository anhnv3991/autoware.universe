<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>component_monitor</name>
  <version>0.45.0</version>
  <description>Monitor the output of components</description>

  <maintainer email="anh.nguyen.2@tier4.jp">Anh Nguyen</maintainer>

  <license>Apache License 2.0</license>

  <author email="anh.nguyen.2@tier4.jp">Anh Nguyen</author>

  <buildtool_depend>ament_cmake_auto</buildtool_depend>
  <buildtool_depend>autoware_cmake</buildtool_depend>

  <depend>autoware_internal_debug_msgs</depend>
  <depend>autoware_perception_msgs</depend>
  <depend>eigen</depend>
  <depend>libpcl-all-dev</depend>
  <depend>pcl_conversions</depend>
  <depend>pcl_ros</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>rosbag2_cpp</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2_geometry_msgs</depend>

  <exec_depend>autoware_launch</exec_depend>
  <exec_depend>reaction_analyzer</exec_depend>
  <exec_depend>tier4_perception_launch</exec_depend>

  <test_depend>ament_cmake_ros</test_depend>
  <test_depend>ament_index_python</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>autoware_lint_common</test_depend>
  <test_depend>autoware_testing</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
